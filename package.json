  "devDependencies": {
    "@carry0987/event-emitter": "^1.4.3",
    "@carry0987/utils": "^3.6.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup": "^4.18.0",
